{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "timers": {
            "maxConcurrency": 1,
            "maxCount": 1
        }
    },
    "functionTimeout": "00:10:00"
}
